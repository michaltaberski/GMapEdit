!!!
%html
  %head
    %meta{ :charset => 'utf-8' }
    %title GMapEdit
    = stylesheet_link_tag "reset", "menu",  "application"
    = javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js"
    = javascript_include_tag "http://maps.google.com/maps/api/js?sensor=false"
    = javascript_include_tag "rails", "json2"   
    = javascript_include_tag "cs/underscore", "cs/gmapedit", "cs/polygon.gmapedit", "cs/polyline.gmapedit", "cs/application"
    = csrf_meta_tag
    %body
      = yield

